{
  "name": "wb-passport",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsc -p .",
    "test": "vitest run",
    "jvalidate:draft": "tsx src/index.ts --validate draft samples/sample-passport-draft.json",
    "jvalidate:sealed": "tsx src/index.ts --validate sealed samples/sample-passport-sealed.json",
    "ingest:dekra": "tsx src/cli/ingest-dekra.ts --in samples/pdfs --out samples/out",
    "validate:draft": "tsx src/cli/validate.ts draft",
    "validate:sealed": "tsx src/cli/validate.ts sealed",
    "seal": "tsx src/cli/seal.ts",
    "extract": "tsx src/cli/extract.ts",
    "verify": "tsx src/cli/verify.ts"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "commander": "^12.1.0",
    "date-fns": "^3.6.0",
    "pdf-parse": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "tsx": "^4.16.2",
    "typescript": "^5.5.4",
    "vitest": "^3.2.4"
  }
}